{
  "name": "mind-trader",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "DATABASE_URL=\"file:./dev.db\" npx tsx prisma/seed.ts",
    "db:studio": "DATABASE_URL=\"file:./dev.db\" npx prisma studio --port 5555",
    "db:push": "DATABASE_URL=\"file:./dev.db\" npx prisma db push",
    "db:reset": "DATABASE_URL=\"file:./dev.db\" npx prisma db push --accept-data-loss && pnpm run db:seed"
  },
